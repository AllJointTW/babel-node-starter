{
  "name": "babel-node-starter",
  "version": "1.0.0",
  "description": "babel nodejs starter",
  "author": "Tristan Teufel",
  "homepage": "teufel-it.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/firsttris/babel-node-starter.git"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec npm run babel-node",
    "check-types": "flow src",
    "test": "jest --coverage",
    "test-watch": "jest --coverage --watchAll",
    "babel-watch": "babel src --watch --out-dir dist",
    "babel-build": "babel src --out-dir dist",
    "babel-node": "babel-node src --presets=env",
    "webpack-build": "webpack --config webpack.config.js",
    "docker-build": "docker-compose build",
    "docker-up": "docker-compose up"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "concurrently": "^3.5.0",
    "flow-bin": "^0.48.0",
    "flow-typed": "^2.1.2",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "webpack": "^3.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
